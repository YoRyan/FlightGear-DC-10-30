				<property>controls/flight/spoilers</property>
				<step>0.2</step>
				<max>1</max>
			</binding>
		</action>
		<action>
			<button>1</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>controls/flight/spoilers</property>
