	<!-- Parking brake -->
	<animation>
		<type>pick</type>
		<object-name>ParkingBrake</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>controls/gear/brake-parking</property>
			</binding>
		</action>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>ParkingBrake</object-name>
		<property>controls/gear/brake-parking</property>
		<interpolation>
			<entry><ind>0</ind><dep>-18</dep></entry>
			<entry><ind>1</ind><dep>29</dep></entry>
		</interpolation>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>-2.548</x-m>
			<y-m>0</y-m>
			<z-m>0.522</z-m>
		</center>
	</animation>

	<!-- Elevator trim -->
